<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Padre
 *
 * @ORM\Table(name="padre")
 * @ORM\Entity
 */
class Padre
{
    /**
     * @var integer
     *
     * @ORM\Column(name="idPadre", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $idpadre;

    /**
     * @var string
     *
     * @ORM\Column(name="nombrePadre", type="string", length=70, nullable=true)
     */
    private $nombrepadre;

    /**
     * @var string
     *
     * @ORM\Column(name="lugarTrabajoP", type="string", length=45, nullable=true)
     */
    private $lugartrabajop;

    /**
     * @var string
     *
     * @ORM\Column(name="telefonoTrabajoPadre", type="string", length=8, nullable=true)
     */
    private $telefonotrabajopadre;

    /**
     * @var string
     *
     * @ORM\Column(name="nombreMadre", type="string", length=70, nullable=true)
     */
    private $nombremadre;

    /**
     * @var string
     *
     * @ORM\Column(name="lugarTrabajoM", type="string", length=45, nullable=true)
     */
    private $lugartrabajom;

    /**
     * @var string
     *
     * @ORM\Column(name="telefonoTrabajoMadre", type="string", length=8, nullable=true)
     */
    private $telefonotrabajomadre;



    /**
     * Get idpadre
     *
     * @return integer
     */
    public function getIdpadre()
    {
        return $this->idpadre;
    }

    /**
     * Set nombrepadre
     *
     * @param string $nombrepadre
     *
     * @return Padre
     */
    public function setNombrepadre($nombrepadre)
    {
        $this->nombrepadre = $nombrepadre;

        return $this;
    }

    /**
     * Get nombrepadre
     *
     * @return string
     */
    public function getNombrepadre()
    {
        return $this->nombrepadre;
    }

    /**
     * Set lugartrabajop
     *
     * @param string $lugartrabajop
     *
     * @return Padre
     */
    public function setLugartrabajop($lugartrabajop)
    {
        $this->lugartrabajop = $lugartrabajop;

        return $this;
    }

    /**
     * Get lugartrabajop
     *
     * @return string
     */
    public function getLugartrabajop()
    {
        return $this->lugartrabajop;
    }

    /**
     * Set telefonotrabajopadre
     *
     * @param string $telefonotrabajopadre
     *
     * @return Padre
     */
    public function setTelefonotrabajopadre($telefonotrabajopadre)
    {
        $this->telefonotrabajopadre = $telefonotrabajopadre;

        return $this;
    }

    /**
     * Get telefonotrabajopadre
     *
     * @return string
     */
    public function getTelefonotrabajopadre()
    {
        return $this->telefonotrabajopadre;
    }

    /**
     * Set nombremadre
     *
     * @param string $nombremadre
     *
     * @return Padre
     */
    public function setNombremadre($nombremadre)
    {
        $this->nombremadre = $nombremadre;

        return $this;
    }

    /**
     * Get nombremadre
     *
     * @return string
     */
    public function getNombremadre()
    {
        return $this->nombremadre;
    }

    /**
     * Set lugartrabajom
     *
     * @param string $lugartrabajom
     *
     * @return Padre
     */
    public function setLugartrabajom($lugartrabajom)
    {
        $this->lugartrabajom = $lugartrabajom;

        return $this;
    }

    /**
     * Get lugartrabajom
     *
     * @return string
     */
    public function getLugartrabajom()
    {
        return $this->lugartrabajom;
    }

    /**
     * Set telefonotrabajomadre
     *
     * @param string $telefonotrabajomadre
     *
     * @return Padre
     */
    public function setTelefonotrabajomadre($telefonotrabajomadre)
    {
        $this->telefonotrabajomadre = $telefonotrabajomadre;

        return $this;
    }

    /**
     * Get telefonotrabajomadre
     *
     * @return string
     */
    public function getTelefonotrabajomadre()
    {
        return $this->telefonotrabajomadre;
    }
}
