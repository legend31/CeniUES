<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Padre
 *
 * @ORM\Table(name="padre")
 * @ORM\Entity
 */
class Padre
{
    /**
     * @var integer
     *
     * @ORM\Column(name="idPadre", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $idpadre;

    /**
     * @var string
     *
     * @ORM\Column(name="nombrePadre", type="string", length=70, nullable=true)
     */
    private $nombrepadre;

    /**
     * @var string
     *
     * @ORM\Column(name="lugarTrabajoP", type="string", length=45, nullable=true)
     */
    private $lugartrabajop;

    /**
     * @var string
     *
     * @ORM\Column(name="telefonoTrabajoPadre", type="string", length=8, nullable=true)
     */
    private $telefonotrabajopadre;

    /**
     * @var string
     *
     * @ORM\Column(name="nombreMadre", type="string", length=70, nullable=true)
     */
    private $nombremadre;

    /**
     * @var string
     *
     * @ORM\Column(name="lugarTrabajoM", type="string", length=45, nullable=true)
     */
    private $lugartrabajom;

    /**
     * @var string
     *
     * @ORM\Column(name="telefonoTrabajoMadre", type="string", length=8, nullable=true)
     */
    private $telefonotrabajomadre;


}

