<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Padre" table="padre">
    <id name="idpadre" type="integer" column="idPadre">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombrepadre" type="string" column="nombrePadre" length="70" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lugartrabajop" type="string" column="lugarTrabajoP" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefonotrabajopadre" type="string" column="telefonoTrabajoPadre" length="8" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombremadre" type="string" column="nombreMadre" length="70" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lugartrabajom" type="string" column="lugarTrabajoM" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefonotrabajomadre" type="string" column="telefonoTrabajoMadre" length="8" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
