<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Evaluacion" table="evaluacion">
    <id name="idevaluacion" type="integer" column="idEvaluacion">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreevaluacion" type="string" column="nombreEvaluacion" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ponderacion" type="float" column="ponderacion" precision="10" scale="0" nullable="true"/>
  </entity>
</doctrine-mapping>
