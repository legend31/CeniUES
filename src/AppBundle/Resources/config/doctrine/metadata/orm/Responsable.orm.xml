<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Responsable" table="responsable">
    <id name="idresponsable" type="integer" column="idResponsable">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreresponsable" type="string" column="nombreResponsable" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="parentesco" type="string" column="parentesco" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono" type="string" column="telefono" length="8" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
